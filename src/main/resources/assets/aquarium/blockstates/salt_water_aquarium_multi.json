{
  "multipart": [
    {
      "when": {
        "down": "true",
        "facing": "north"
      },
      "apply": {
        "model": "aquarium:blocks/bottom_side_fish_tank"
      }
    },
    {
      "when": {
        "down": "true",
        "facing": "south"
      },
      "apply": {
        "model": "aquarium:blocks/bottom_side_fish_tank",
        "y": 180
      }
    },
    {
      "when": {
        "down": "true",
        "facing": "west"
      },
